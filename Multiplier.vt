`timescale 1ns / 100ps
module FFTtest ();

reg [31:0] A;
reg [31:0] B;
wire C;

FFT_f1 FFT_f1 ( .A(A), .B(B), .C());

initial begin
    $display("Ranning testbench");
    #5 A = 32'b0_01111110_11000000000000000000000;B = 32'b0_00000000_10110000000000000000000;
    #5 A = 32'b0_01111111_11000000000000000000000;B = 32'b0_00000000_10110000000000000000000;
    #5 A = 32'b0_01100000_11000000000000000000000;B = 32'b0_00001111_10110000000000000000000;
    #5 A = 32'b0_10000000_00000000000000000000000;B = 32'b0_10000000_00000000000000000000000;
    #5 A = 32'b0_00111100_10000000000000000000000;B = 32'b0_00111111_11110000000000000000000;
    #5 A = 32'b0_10000000_11000000000000000000000;B = 32'b0_00000000_10110000000000000000000;    
    $display("Stoping testbench");
    #100 $stop();  
end

endmodule